<view class="padding-top"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="card-menu cu-list menu"><view class="cu-item "><view class="content"><text class="text-orange cuIcon-roundrightfill"></text>新增收支</view></view><block wx:if="{{false}}"><view class="cu-form-group "><view class="title">交易编号<text class="text-red"></text></view><input placeholder="交易编号（非必须，后台会自动生成）" name="sn" disabled="disabled"/></view></block><view class="cu-form-group "><view class="title">收支选择</view><text class="text-red">*</text><picker value="{{inout}}" range="{{picker}}" name="iosel" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(inout>-1?picker[inout]:'请选择收入或支出')+''}}</view></picker></view><view class="cu-form-group "><view class="title">收支分类</view><text class="text-red">*</text><picker value="{{inoutValue}}" range="{{ioFilterRange}}" name="iofilter" data-event-opts="{{[['change',[['InOutFilterChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(inoutValue>-1?ioFilterRange[inoutValue]:'请选择收入或支出的类别')+''}}</view></picker></view><view class="cu-form-group "><view class="title">收支金额</view><text class="text-red">*</text><input placeholder="单位:元" name="iomoney" type="digit"/></view><view class="cu-form-group "><view class="title">支付方式</view><text class="text-red">*</text><picker value="{{typeindex}}" range="{{typerange}}" name="iotype" data-event-opts="{{[['change',[['IoTypeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(typeindex>-1?typerange[typeindex]:'请选择支付方式')+''}}</view></picker></view><view class="cu-form-group "><view class="title">备注信息</view><input placeholder="备注" name="desc"/></view><view class="cu-form-group "><button class="cu-btn bg-blue shadow" form-type="submit" loading="{{loading}}">提交信息</button></view></view></form></view>