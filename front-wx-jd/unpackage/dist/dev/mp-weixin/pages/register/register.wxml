<view class="zai-box"><image class="zai-logo" src="../../static/zaizai-login/logo.png" mode="aspectFit"></image><view class="zai-title"></view><form><view class="cu-form-group"><view class="title">真实姓名</view><input placeholder-class="{{true}}" placeholder="请输入姓名" name="input" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机号码</view><input placeholder="请输入手机号" placeholder-class="{{true}}" name="input" data-event-opts="{{[['input',[['__set_model',['','mobile','$event',[]]]]]]}}" value="{{mobile}}" bindinput="__e"/><view class="cu-capsule radius"><view class="cu-tag bg-blue ">+86</view><view class="cu-tag line-blue">中国</view></view></view><view class="cu-form-group"><view class="title">公司编号</view><input placeholder-class="{{true}}" placeholder="请输入公司ID" name="input" data-event-opts="{{[['input',[['__set_model',['','companyid','$event',[]]]]]]}}" value="{{companyid}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">公司名称</view><input placeholder-class="{{true}}" placeholder="请输入公司名称" name="input" data-event-opts="{{[['input',[['__set_model',['','companyname','$event',[]]]]]]}}" value="{{companyname}}" bindinput="__e"/></view><view class="btn-group"><button data-event-opts="{{[['tap',[['login']]]]}}" class="cu-btn bg-gradual-blue round" bindtap="__e">立即注册</button></view></form></view>